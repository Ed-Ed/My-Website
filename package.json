{
  "name": "my-website",
  "private": true,
  "scripts": {
    "dev::ssr": "cross-env NODE_ENV=development MODE=ssr webpack",
    "dev::client": "cross-env NODE_ENV=development MODE=client yarn start",
    "prod::ssr": "cross-env NODE_ENV=production MODE=ssr webpack",
    "prod::client": "cross-env NODE_ENV=production MODE=client webpack",
    "clean": "rm -rf dist",
    "nodemon": "nodemon --watch dist/ssr/index.js --exec yarn dev::client",
    "dev": "concurrently \"yarn dev::ssr --watch\" \"yarn nodemon\"",
    "build": "yarn clean && yarn prod::ssr && yarn prod::client",
    "qa::lint": "eslint src/**/* --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "qa::format": "prettier --check src/**/*.{ts,tsx} --write",
    "qa": "yarn qa::format && yarn qa::lint",
    "deploy": "yarn qa && yarn build && gcloud app deploy",
    "start": "node dist/ssr/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.2",
    "@svgr/webpack": "^5.5.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-lazyload": "^3.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-middleware": "^3.7.3",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.1.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@edwardandrewb/elements": "^0.0.15",
    "@loadable/component": "^5.13.2",
    "@loadable/server": "^5.14.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazyload": "^3.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0",
    "webpack": "^4.46.0",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
