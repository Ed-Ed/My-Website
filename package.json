{
  "name": "my-website",
  "private": true,
  "scripts": {
    "dev::ssr": "cross-env NODE_ENV=development MODE=ssr webpack",
    "dev::client": "cross-env NODE_ENV=development MODE=client yarn start",
    "prod::ssr": "cross-env NODE_ENV=production MODE=ssr webpack",
    "prod::client": "cross-env NODE_ENV=production MODE=client webpack",
    "clean": "rm -rf dist",
    "nodemon": "nodemon --watch dist/ssr/index.js --exec yarn dev::client",
    "dev": "concurrently \"yarn dev::ssr --watch\" \"yarn nodemon\"",
    "build": "yarn clean && yarn prod::ssr && yarn prod::client",
    "start": "node dist/ssr/index.js",
    "lint": "eslint src/**/* --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "deploy": "yarn lint && yarn build && gcloud app deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.13.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-lazyload": "^3.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-env": "^1.15.3",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^6.1.1",
    "nodemon": "^2.0.5",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@edwardandrewb/elements": "^0.0.15",
    "@loadable/component": "^5.13.2",
    "@loadable/server": "^5.13.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazyload": "^3.0.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0"
  }
}
